            <text pos="60,50">root</text>
            <text pos="0,75">密码:</text>
            <text pos="60,75">alpine（初始密码）</text>
          </window>
          <button pos="300,85,@150,@30" name="btn_generate" skin="blue_btn" colorText="#ffffff" drawFocusRect="0">一键生成 XShell 配置</button>
          <window pos="0,-75,-0,@1" colorBkgnd="#e4e4e4"/>
          <button pos="|0,-57,@120,@40" name="btn_stop" offset="-0.5,0" drawFocusRect="0">关闭 SSH 通道</button>
        </page>
      </tabctrl>

    </window>
  </root>
</SOUI>
