        <edit name="edit_url" pos="{0,[10,-30,@20" skin="normal_edit" font="adding:-2" inset="5,0,5,0" cueText="地址" cueColor="#9a9a9a"></edit>
        <imgbtn id="IDOK" pos="-180,-34,@80,@24" font="adding:-2" animate="1" colorTextDisable="#cccccc">保存</imgbtn>
        <imgbtn id="IDCANCEL" pos="[10,-34,@80,@24" font="adding:-2" animate="1" colorTextDisable="#cccccc">取消</imgbtn>
