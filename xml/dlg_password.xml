<SOUI title="mesagebox" width="440" height="280" appwin="0" resize="0" translucent="1" trCtx="">
  <style>
    <class name="normalbtn" skin="_skin.sys.btn.normal" font="" colorText="#333333" colorTextDisable="#91a7c0" textMode="0x25" cursor="hand" margin-x="0"/>
  </style>
  <root skin="_skin.sys.wnd.bkgnd">
    <window pos="11,11,-11,-11" colorBkgnd="#FFFFFF" >
    <caption id="101" pos="0,0,-0,@42">
      <text pos="20, 0, @300, @42"  align="left" name="msgtitle" >请输入 iTunes 备份密码</text>
      <imgbtn id="2" skin="_skin.sys.btn.close" pos="-35,10" tip="close"/>
      <window pos="1, 42, -1, @1" colorBkgnd="#E4E4E4"/>
    </caption>

    <window pos="0,[30,-0,@100">
      <text pos=" 0, 0, @105, @30" align="right">Password:</text>
      <edit name="edit_passwd" pos="110, 0, @210, @30" skin="normal_edit" inset="5,0,5,0" password="1"></edit>
      <text pos="0,45,@105,@30" name="txt_dev" align="right" visible="0">设备:</text>
      <combobox name="combo_dev" visible="0" pos="110,45,@210,@30" dropDown="1" dropHeight="100" skin="normal_edit" dotted="0" animateTime="200" drawFocusRect="0" inset="5,0,5,0">
        <liststyle class="cls_combo_list" itemHeight="30"/>
        <editstyle inset="5,0,5,0" colorText="@color/text_body" align="center" colorBkgnd="#FFFFFF"/>
      </combobox>
    </window>
    
    <button name="btn_ok" pos="130, -50, @75, @30">Ok</button>
    <button name="btn_cancel" pos="220, -50, @75, @30" id="IDCANCEL">Cancel</button>
    
  </window>
  </root>
</SOUI>